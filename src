-- // ServerStorage.PinnacleUI.Main.Security.StudioProtect.whitelistStudio \\ --
local function SCRIPT_8()
local script = UI["8"]
	if game:GetService("RunService"):IsStudio() and game.PlaceId ~= 128385029419361 then
		script.Parent.Parent.Parent.Parent:Destroy()
	end
	
end
task.spawn(SCRIPT_8)
-- // ServerStorage.PinnacleUI.Main.UI.top.TextLabel.LocalScript \\ --
local function SCRIPT_f()
local script = UI["f"]
	local player = game.Players.LocalPlayer
	
	script.Parent.Text = "Welcome, "..player.Name.."."
end
task.spawn(SCRIPT_f)
-- // ServerStorage.PinnacleUI.Main.UI.top.ImageLabel.LocalScript \\ --
local function SCRIPT_13()
local script = UI["13"]
	local player = game.Players.LocalPlayer
	local imageLabel = script.Parent
	
	local success, thumbnail = pcall(function()
		return game.Players:GetUserThumbnailAsync(player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
	end)
	
	if success then
		imageLabel.Image = thumbnail
	end
	
end
task.spawn(SCRIPT_13)
-- // ServerStorage.PinnacleUI.Main.SectionButtons.combat.LocalScript \\ --
local function SCRIPT_1c()
local script = UI["1c"]
	local pages = script.Parent.Parent.Parent.Pages
	local except = pages.combat
	
	script.Parent.MouseButton1Click:Connect(function()
		for _, obj in ipairs(script.Parent.Parent.Parent.Pages:GetChildren()) do
			if obj:IsA("Frame") then
				obj.Visible = false
			end
			except.Visible = true
		end
	end)
end
task.spawn(SCRIPT_1c)
-- // ServerStorage.PinnacleUI.Main.SectionButtons.visuals.LocalScript \\ --
local function SCRIPT_1f()
local script = UI["1f"]
	local pages = script.Parent.Parent.Parent.Pages
	local except = pages.visuals
	
	script.Parent.MouseButton1Click:Connect(function()
		for _, obj in ipairs(script.Parent.Parent.Parent.Pages:GetChildren()) do
			if obj:IsA("Frame") then
				obj.Visible = false
			end
			except.Visible = true
		end
	end)
end
task.spawn(SCRIPT_1f)
-- // ServerStorage.PinnacleUI.Main.KeyBind \\ --
local function SCRIPT_20()
local script = UI["20"]
	game:GetService("UserInputService").InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.RightShift then
			script.Parent.Visible = not script.Parent.Visible
		end
	end)
end
task.spawn(SCRIPT_20)
-- // ServerStorage.PinnacleUI.Main.Pages.combat.Aimlock.TextButton.Animate \\ --
local function SCRIPT_27()
local script = UI["27"]
	local TweenService = game:GetService("TweenService")
	
	local ActivateOnce = false -- true for one-time activation
	
	local button = script.Parent
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(0, 40, 0, 20)
	frame.Position = UDim2.new(0.5, 0, 0.5, 0)
	frame.AnchorPoint = Vector2.new(0.5, 0.5)
	frame.BackgroundColor3 = Color3.fromRGB(22, 22, 24)
	frame.Parent = button
	
	local mainCorner = Instance.new("UICorner")
	mainCorner.CornerRadius = UDim.new(1, 0)
	mainCorner.Parent = frame
	
	local layerEffect = Instance.new("Frame")
	layerEffect.Size = UDim2.new(1, 0, 1, 0)
	layerEffect.Position = UDim2.new(0.5, 0, 0.5, 0)
	layerEffect.AnchorPoint = Vector2.new(0.5, 0.5)
	layerEffect.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	layerEffect.BackgroundTransparency = 0.98
	layerEffect.Parent = frame
	
	local layerGradient = Instance.new("UIGradient")
	layerGradient.Transparency = NumberSequence.new({
		NumberSequenceKeypoint.new(0, 0.98),
		NumberSequenceKeypoint.new(0.5, 0.96),
		NumberSequenceKeypoint.new(1, 0.98)
	})
	layerGradient.Parent = layerEffect
	
	local layerCorner = Instance.new("UICorner")
	layerCorner.CornerRadius = UDim.new(1, 0)
	layerCorner.Parent = layerEffect
	
	local ambientGlow = Instance.new("Frame")
	ambientGlow.Size = UDim2.new(1, 4, 1, 4)
	ambientGlow.Position = UDim2.new(0.5, 0, 0.5, 0)
	ambientGlow.AnchorPoint = Vector2.new(0.5, 0.5)
	ambientGlow.BackgroundColor3 = Color3.fromRGB(100, 100, 120)
	ambientGlow.BackgroundTransparency = 0.94
	ambientGlow.Parent = frame
	
	local ambientCorner = Instance.new("UICorner")
	ambientCorner.CornerRadius = UDim.new(1, 0)
	ambientCorner.Parent = ambientGlow
	
	local secondaryGlow = Instance.new("Frame")
	secondaryGlow.Size = UDim2.new(1, 6, 1, 6)
	secondaryGlow.Position = UDim2.new(0.5, 0, 0.5, 0)
	secondaryGlow.AnchorPoint = Vector2.new(0.5, 0.5)
	secondaryGlow.BackgroundColor3 = Color3.fromRGB(100, 100, 120)
	secondaryGlow.BackgroundTransparency = 0.97
	secondaryGlow.Parent = frame
	
	local secondaryCorner = Instance.new("UICorner")
	secondaryCorner.CornerRadius = UDim.new(1, 0)
	secondaryCorner.Parent = secondaryGlow
	
	local circle = Instance.new("Frame")
	circle.Size = UDim2.new(0, 16, 0, 16)
	circle.Position = UDim2.new(0, 2, 0.5, 0)
	circle.AnchorPoint = Vector2.new(0, 0.5)
	circle.BackgroundColor3 = Color3.fromRGB(245, 245, 245)
	circle.Parent = frame
	
	local innerShadow = Instance.new("Frame")
	innerShadow.Size = UDim2.new(1, -2, 1, -2)
	innerShadow.Position = UDim2.new(0.5, 0, 0.5, 0)
	innerShadow.AnchorPoint = Vector2.new(0.5, 0.5)
	innerShadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	innerShadow.BackgroundTransparency = 0.96
	innerShadow.Parent = circle
	
	local innerShadowCorner = Instance.new("UICorner")
	innerShadowCorner.CornerRadius = UDim.new(1, 0)
	innerShadowCorner.Parent = innerShadow
	
	local circleShadow = Instance.new("Frame")
	circleShadow.Size = UDim2.new(1, 2, 1, 2)
	circleShadow.Position = UDim2.new(0.5, 0, 0.5, 0)
	circleShadow.AnchorPoint = Vector2.new(0.5, 0.5)
	circleShadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	circleShadow.BackgroundTransparency = 0.85
	circleShadow.ZIndex = -1
	circleShadow.Parent = circle
	
	local circleShadowCorner = Instance.new("UICorner")
	circleShadowCorner.CornerRadius = UDim.new(1, 0)
	circleShadowCorner.Parent = circleShadow
	
	local circleCorner = Instance.new("UICorner")
	circleCorner.CornerRadius = UDim.new(1, 0)
	circleCorner.Parent = circle
	
	local circleGradient = Instance.new("UIGradient")
	circleGradient.Color = ColorSequence.new({
		ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
		ColorSequenceKeypoint.new(0.4, Color3.fromRGB(250, 250, 250)),
		ColorSequenceKeypoint.new(0.7, Color3.fromRGB(245, 245, 245)),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(240, 240, 240))
	})
	circleGradient.Rotation = 45
	circleGradient.Parent = circle
	
	local circleHighlight = Instance.new("Frame")
	circleHighlight.Size = UDim2.new(1, 0, 0.5, 0)
	circleHighlight.Position = UDim2.new(0, 0, 0, 0)
	circleHighlight.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	circleHighlight.BackgroundTransparency = 0.92
	circleHighlight.Parent = circle
	
	local highlightGradient = Instance.new("UIGradient")
	highlightGradient.Transparency = NumberSequence.new({
		NumberSequenceKeypoint.new(0, 0.85),
		NumberSequenceKeypoint.new(0.5, 0.92),
		NumberSequenceKeypoint.new(1, 0.96)
	})
	highlightGradient.Parent = circleHighlight
	
	local highlightCorner = Instance.new("UICorner")
	highlightCorner.CornerRadius = UDim.new(1, 0)
	highlightCorner.Parent = circleHighlight
	
	local toggle = false
	local hasActivated = false
	
	local function breatheEffect()
		while true do
			local breatheIn = TweenService:Create(ambientGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.92,
				Size = UDim2.new(1, 8, 1, 8)
			})
			local secondaryBreatheIn = TweenService:Create(secondaryGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.95,
				Size = UDim2.new(1, 12, 1, 12)
			})
			breatheIn:Play()
			secondaryBreatheIn:Play()
			task.wait(4)
	
			local breatheOut = TweenService:Create(ambientGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.94,
				Size = UDim2.new(1, 4, 1, 4)
			})
			local secondaryBreatheOut = TweenService:Create(secondaryGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.97,
				Size = UDim2.new(1, 6, 1, 6)
			})
			breatheOut:Play()
			secondaryBreatheOut:Play()
			task.wait(4)
		end
	end
	
	coroutine.wrap(breatheEffect)()
	
	button.MouseButton1Click:Connect(function()
		if ActivateOnce and hasActivated then return end
	
		toggle = not toggle
		hasActivated = true
	
		local circlePos = toggle and UDim2.new(1, -18, 0.5, 0) or UDim2.new(0, 2, 0.5, 0)
		local bgColor = toggle and Color3.fromRGB(40, 40, 45) or Color3.fromRGB(22, 22, 24)
		local glowColor = toggle and Color3.fromRGB(130, 130, 150) or Color3.fromRGB(100, 100, 120)
	
		local tweenInfo = TweenInfo.new(0.6, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
		local circleTween = TweenService:Create(circle, tweenInfo, {Position = circlePos})
		local colorTween = TweenService:Create(frame, tweenInfo, {BackgroundColor3 = bgColor})
		local glowColorTween = TweenService:Create(ambientGlow, tweenInfo, {BackgroundColor3 = glowColor})
		local secondaryGlowTween = TweenService:Create(secondaryGlow, tweenInfo, {BackgroundColor3 = glowColor})
	
		local squishTween = TweenService:Create(circle, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
			Size = toggle and UDim2.new(0, 18, 0, 14) or UDim2.new(0, 14, 0, 18)
		})
		local unsquishTween = TweenService:Create(circle, TweenInfo.new(0.4, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out), {
			Size = UDim2.new(0, 16, 0, 16)
		})
	
		squishTween:Play()
		task.wait(0.15)
		unsquishTween:Play()
		circleTween:Play()
		colorTween:Play()
		glowColorTween:Play()
		secondaryGlowTween:Play()
	end)
	
	button.AutoButtonColor = false
	button.BackgroundTransparency = 1
	button.Size = UDim2.new(0, 40, 0, 20)
end
task.spawn(SCRIPT_27)
-- // ServerStorage.PinnacleUI.Main.Pages.combat.Backtrack.TextButton.Animate \\ --
