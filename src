			local breatheIn = TweenService:Create(ambientGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.92,
				Size = UDim2.new(1, 8, 1, 8)
			})
			local secondaryBreatheIn = TweenService:Create(secondaryGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.95,
				Size = UDim2.new(1, 12, 1, 12)
			})
			breatheIn:Play()
			secondaryBreatheIn:Play()
			task.wait(4)
	
			local breatheOut = TweenService:Create(ambientGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.94,
				Size = UDim2.new(1, 4, 1, 4)
			})
			local secondaryBreatheOut = TweenService:Create(secondaryGlow, TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				BackgroundTransparency = 0.97,
				Size = UDim2.new(1, 6, 1, 6)
			})
			breatheOut:Play()
			secondaryBreatheOut:Play()
			task.wait(4)
		end
	end
	
	coroutine.wrap(breatheEffect)()
	
	button.MouseButton1Click:Connect(function()
		if ActivateOnce and hasActivated then return end
	
		toggle = not toggle
		hasActivated = true
	
		local circlePos = toggle and UDim2.new(1, -18, 0.5, 0) or UDim2.new(0, 2, 0.5, 0)
		local bgColor = toggle and Color3.fromRGB(40, 40, 45) or Color3.fromRGB(22, 22, 24)
		local glowColor = toggle and Color3.fromRGB(130, 130, 150) or Color3.fromRGB(100, 100, 120)
	
		local tweenInfo = TweenInfo.new(0.6, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
		local circleTween = TweenService:Create(circle, tweenInfo, {Position = circlePos})
		local colorTween = TweenService:Create(frame, tweenInfo, {BackgroundColor3 = bgColor})
		local glowColorTween = TweenService:Create(ambientGlow, tweenInfo, {BackgroundColor3 = glowColor})
		local secondaryGlowTween = TweenService:Create(secondaryGlow, tweenInfo, {BackgroundColor3 = glowColor})
	
		local squishTween = TweenService:Create(circle, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
			Size = toggle and UDim2.new(0, 18, 0, 14) or UDim2.new(0, 14, 0, 18)
		})
		local unsquishTween = TweenService:Create(circle, TweenInfo.new(0.4, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out), {
			Size = UDim2.new(0, 16, 0, 16)
		})
	
		squishTween:Play()
		task.wait(0.15)
		unsquishTween:Play()
		circleTween:Play()
		colorTween:Play()
		glowColorTween:Play()
		secondaryGlowTween:Play()
	end)
	
	button.AutoButtonColor = false
	button.BackgroundTransparency = 1
	button.Size = UDim2.new(0, 40, 0, 20)
end
task.spawn(SCRIPT_2c)
-- // ServerStorage.PinnacleUI.Main.Pages.combat.Backtrack.TextButton.script \\ --
local function SCRIPT_2d()
local script = UI["2d"]
	local button = script.Parent
	local textBox = script.Parent.Parent.TextBox.ms
	local active = false
	local localPlayer = game.Players.LocalPlayer
	local players = game.Players
	
	button.MouseButton1Click:Connect(function()
		active = not active
		if active then
			print("on")
			while active do
				for _, player in ipairs(players:GetPlayers()) do
					if player ~= localPlayer and player.Character and player.Character:FindFirstChild("Head") then
						local head = player.Character.Head
						head.Anchored = true
						task.wait(textBox.Value / 1000)
						head.Anchored = false
					end
				end
				task.wait(math.random(1, 3))
			end
		else
			print("off")
		end
	end)
	
	textBox:GetPropertyChangedSignal("Value"):Connect(function()
		print(textBox.Value)
	end)
	
end
task.spawn(SCRIPT_2d)
-- // ServerStorage.PinnacleUI.Main.Pages.combat.Backtrack.TextBox.mschange \\ --
local function SCRIPT_31()
local script = UI["31"]
	script.Parent.Changed:Connect(function()
		if #script.Parent.Text > 4 then
			script.Parent.Text = string.sub(script.Parent.Text, 1, 4)
		end
	
		local newText = script.Parent.Text:gsub("%D", "")
		if newText ~= script.Parent.Text then
			script.Parent.Text = newText
		end
	
		if script.Parent.ms and script.Parent.ms:IsA("NumberValue") then
			script.Parent.ms.Value = tonumber(script.Parent.Text) or 0
		end
	end)
	
end
task.spawn(SCRIPT_31)
