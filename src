-- // ServerStorage.PinnacleUI.Main.Pages.combat.Hitbox.TextButton.script \\ --
local function SCRIPT_37()
local script = UI["37"]
	local button = script.Parent
	local localPlayer = game.Players.LocalPlayer
	local active = false
	
	button.MouseButton1Click:Connect(function()
		active = true
		if active == true then
			return
		end
		_G.HeadSize = 50
		_G.Disabled = false
	
		local Players = game:GetService('Players')
		local LocalPlayer = Players.LocalPlayer
		local createdHitboxes = {}
	
		local function isPlayerInVehicle(character)
			for _, child in pairs(character:GetDescendants()) do
				if child:IsA("VehicleSeat") or child:IsA("Seat") then
					return true
				end
			end
			return false
		end
	
		local function createHitbox(player)
			local character = player.Character
			if not character then return end
	
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if not humanoidRootPart then return end
	
			humanoidRootPart.Size = Vector3.new(_G.HeadSize, _G.HeadSize, _G.HeadSize)
			humanoidRootPart.Transparency = 0.7
			humanoidRootPart.BrickColor = BrickColor.new("Really blue")
			humanoidRootPart.Material = "Neon"
			humanoidRootPart.CanCollide = false
			createdHitboxes[player] = humanoidRootPart
		end
	
		local function removeHitbox(player)
			if createdHitboxes[player] then
				local humanoidRootPart = createdHitboxes[player]
				humanoidRootPart.Size = Vector3.new(2, 2, 1)
				humanoidRootPart.Transparency = 0
				humanoidRootPart.BrickColor = BrickColor.new("Medium stone grey")
				humanoidRootPart.Material = "Plastic"
				humanoidRootPart.CanCollide = true
				createdHitboxes[player] = nil
			end
		end
	
		local function updateHitbox(player)
			if not player.Character then return end
	
			if player.Team and player.Team ~= LocalPlayer.Team and not isPlayerInVehicle(player.Character) then
				createHitbox(player)
			else
				removeHitbox(player)
			end
	
			local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				if player.Team and player.Team == LocalPlayer.Team or isPlayerInVehicle(player.Character) then
					humanoidRootPart.Transparency = 1
				else
					humanoidRootPart.Transparency = 0.7
				end
			end
		end
	
		local function onCharacterAdded(character)
			local player = Players:GetPlayerFromCharacter(character)
			if not player then return end
	
			local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 10)
			if humanoidRootPart then
				updateHitbox(player)
			end
	
			player:GetPropertyChangedSignal("Team"):Connect(function()
				updateHitbox(player)
			end)
		end
	
		local function onPlayerAdded(player)
			player.CharacterAdded:Connect(onCharacterAdded)
	
			if player.Character then
				onCharacterAdded(player.Character)
			end
		end
	
		local function onPlayerRemoving(player)
			removeHitbox(player)
		end
	
		local function manageHitboxes()
			while true do
				wait(1)
	
				if _G.Disabled then
					for player, _ in pairs(createdHitboxes) do
						removeHitbox(player)
					end
				else
					for _, player in ipairs(Players:GetPlayers()) do
						if player ~= LocalPlayer then
							updateHitbox(player)
						end
					end
				end
			end
		end
	
		Players.PlayerAdded:Connect(onPlayerAdded)
		Players.PlayerRemoving:Connect(onPlayerRemoving)
	
		for _, player in ipairs(Players:GetPlayers()) do
			onPlayerAdded(player)
		end
	
		spawn(manageHitboxes)
	end)
	
end
task.spawn(SCRIPT_37)
