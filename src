		local glowColorTween = TweenService:Create(ambientGlow, tweenInfo, {BackgroundColor3 = glowColor})
		local secondaryGlowTween = TweenService:Create(secondaryGlow, tweenInfo, {BackgroundColor3 = glowColor})
	
		local squishTween = TweenService:Create(circle, TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
			Size = toggle and UDim2.new(0, 18, 0, 14) or UDim2.new(0, 14, 0, 18)
		})
		local unsquishTween = TweenService:Create(circle, TweenInfo.new(0.4, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out), {
			Size = UDim2.new(0, 16, 0, 16)
		})
	
		squishTween:Play()
		task.wait(0.15)
		unsquishTween:Play()
		circleTween:Play()
		colorTween:Play()
		glowColorTween:Play()
		secondaryGlowTween:Play()
	end)
	
	button.AutoButtonColor = false
	button.BackgroundTransparency = 1
	button.Size = UDim2.new(0, 40, 0, 20)
end
task.spawn(SCRIPT_3e)
-- // ServerStorage.PinnacleUI.Main.Pages.visuals.esp.toggle.atual \\ --
local function SCRIPT_3f()
local script = UI["3f"]
	local button = script.Parent
	local active = false
	
	button.MouseButton1Click:Connect(function()
		_G.ESP=false;_G.ESPColor=Color3.fromRGB(255,255,255)pcall(function()local a=Instance.new("Highlight")game:GetService("RunService").RenderStepped:Connect(function()for b,c in pairs(game.Players:GetPlayers())do if not c.Character:FindFirstChild("Highlight")then a.FillTransparency=1;a:Clone().Parent=c.Character;a.OutlineColor=Color3.fromRGB(255,255,255)end;game.Players.PlayerAdded:Connect(function(d)d.CharacterAdded:Connect(function(e)if not e:FindFirstChild("Highlight")then a.FillTransparency=1;a:Clone().Parent=e;a.OutlineColor=Color3.fromRGB(255,255,255)end end)end)end;for b,c in pairs(game.Players:GetPlayers())do local f=c.Character:FindFirstChild("Highlight")f.Enabled=_G.ESP;f.OutlineColor=_G.ESPColor end end)end)local g=loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3"))()local h=g:CreateWindow("Simple ESP")local i=h:CreateFolder("Main")i:Toggle("ESP",function(j)_G.ESP=j end)i:ColorPicker("ESP Color",Color3.fromRGB(255,255,255),function(k)_G.ESPColor=k end)
	end)
	
end
task.spawn(SCRIPT_3f)
-- // ServerStorage.PinnacleUI.Main.Pages.open.TextLabel.rainbow \\ --
local function SCRIPT_49()
local script = UI["49"]
	local textLabel = script.Parent
	local cores = {Color3.fromRGB(255, 215, 0), Color3.fromRGB(0, 255, 255), Color3.fromRGB(255, 182, 193), Color3.fromRGB(255, 127, 80), Color3.fromRGB(144, 238, 144)}
	
	while true do
		for _, cor in ipairs(cores) do
			for i = 1, 10 do
				textLabel.TextColor3 = textLabel.TextColor3:Lerp(cor, 0.1)
				wait(0.05)
			end
			wait(1)
		end
	end
	
end
task.spawn(SCRIPT_49)
-- // ServerStorage.PinnacleUI.Main.Pages.open.LocalScript \\ --
local function SCRIPT_4a()
local script = UI["4a"]
	local except = script.Parent
	local pages = script.Parent.Parent.Parent.Pages
	local uiTop = script.Parent.Parent.Parent.UI.top
	
	for _, obj in ipairs(pages:GetChildren()) do
		if obj:IsA("Frame") then
			obj.Visible = false
		end
	end
	except.Visible = true
	
	uiTop.mountain.Visible = false
	uiTop.watermark.Visible = false
	
	except:GetPropertyChangedSignal("Visible"):Connect(function()
		if except.Visible then
			uiTop.mountain.Visible = false
			uiTop.watermark.Visible = false
		else
			uiTop.mountain.Visible = true
			uiTop.watermark.Visible = true
		end
	end)
	
end
task.spawn(SCRIPT_4a)
