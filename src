-- // ServerStorage.PinnacleUI.Load \\ --
local function SCRIPT_4d()
local script = UI["4d"]
	if game:GetService("RunService"):IsStudio() and game.PlaceId ~= 128385029419361 then
		script.Parent.Enabled = false
		wait(math.random(8, 15))
		script.Parent.Enabled = true
		
		local replicatedStorage = game:GetService("ReplicatedStorage")
		local boolValue = Instance.new("BoolValue")
		boolValue.Name = "LoadPinnacle"
		boolValue.Value = true
		boolValue.Parent = replicatedStorage
	
	else
		print("Pinnacle carregando")
	end
	
end
task.spawn(SCRIPT_4d)

return UI["1"], require;
